# GitHub App Configuration
GITHUB_APP_ID=123456
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END RSA PRIVATE KEY-----"
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/aeamcp_git_registration
DATABASE_SSL=false

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Server Configuration
NODE_ENV=development
PORT=3001
API_BASE_PATH=/api/v1

# Security
JWT_SECRET=your_jwt_secret_here_min_32_chars
ENCRYPTION_KEY=your_32_char_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Analysis Configuration
MAX_REPO_SIZE_MB=100
ANALYSIS_TIMEOUT_MS=300000
TEMP_DIR_TTL_HOURS=1

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log